{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "C/C++: cpp build active file ARM",
      "command": "/usr/bin/arm-linux-gnueabihf-g++-10",
      "args": [
        "-std=c++2a",
        "-g",
        "-Wall",
        "-I${workspaceFolder}",
        "-I${workspaceFolder}/Dev",
        "-I${workspaceFolder}/../LIB.Share",
        "-I${workspaceFolder}/../LIB.Utils",
        "-I/usr/local/boost_1_77_0_ARM",
        "-L/usr/arm-linux-gnueabihf/lib",
        "${workspaceFolder}/main.cpp",
        "-o",
        "${workspaceFolder}/mxinfo",
        "-lpthread"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "type": "cppbuild",
      "label": "C/C++: g++ build active file",
      "command": "/usr/bin/g++-11",
      "args": [
        "-fdiagnostics-color=always",
        "-std=c++2a",
        "-g",
        "-Wall",
        "-I${workspaceFolder}",
        "-I${workspaceFolder}/Dev",
        "-I${workspaceFolder}/../LIB.Share",
        "-I${workspaceFolder}/../LIB.Utils",
        "-I/usr/local/boost_1_77_0",
        "${workspaceFolder}/main.cpp",
        "-o",
        "${workspaceFolder}/mxinfo_dbg",
        "-lpthread"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build",
      "detail": "compiler: /usr/bin/g++",
      "dependsOn": [
        "copy_config_dbg"
      ]
    },
    {
      "label": "copy_config_dbg",
      "command": "cp",
      "args": [
        "-f",
        "${workspaceFolder}/../../mxinfo_dbg.conf.json",
        "."
      ]
    }
  ]
}