        "${workspaceFolder}/../LIB.Utils/utilsFile.cpp",
        "${workspaceFolder}/../LIB.Utils/utilsPath.cpp",
        "${workspaceFolder}/../LIB.Utils/utilsWeb.cpp",
        "-o",
        "${workspaceFolder}/mxspy",
        "-lpthread",
        "-lcrypto",
        "-lssl"
